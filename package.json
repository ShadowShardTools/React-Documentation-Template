{
  "name": "react-documentation-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://shadowshardtools.github.io/React-Documentation-Template/",
  "scripts": {
    "generate:prism": "node src/services/GeneratePrismLanguages.js",
    "generate:index": "node src/services/GenerateIndex.js",
    "dev": "npm run generate:prism && npm run generate:index && vite",
    "build": "npm run generate:prism && npm run generate:index && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.7.12",
    "@splidejs/splide": "^4.1.4",
    "@tailwindcss/vite": "^4.1.6",
    "clsx": "^2.1.1",
    "desmos": "^1.5.4",
    "katex": "^0.16.22",
    "lucide-react": "^0.509.0",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-compare-image": "^3.5.5",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.6.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/culori": "^4.0.0",
    "@types/desmos": "^1.11.0",
    "@types/node": "^24.0.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
