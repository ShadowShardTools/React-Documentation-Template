{
  "name": "sst-docs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "https://shadowshardtools.github.io/SST-Docs/",
  "license": "MIT",
  "repository": "github:ShadowShardTools/SST-Docs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "generate": "run-p generate:*",
    "generate:blocks": "node src/services/generators/GenerateBlockImports.mjs",
    "generate:index": "node src/services/generators/GenerateIndex.mjs",
    "generate:prism": "node src/services/generators/GeneratePrismLanguages.mjs",
    "dev": "npm run generate && vite",
    "build": "npm run generate && tsc -b && vite build",
    "start": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "prebuild": "npm run format",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.7.12",
    "@splidejs/splide": "^4.1.4",
    "clsx": "^2.1.1",
    "desmos": "^1.5.4",
    "katex": "^0.16.22",
    "lucide-react": "^0.509.0",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-compare-image": "^3.5.5",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.6.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.6"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.6",
    "@types/desmos": "^1.11.0",
    "@types/node": "^24.0.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "fast-glob": "^3.3.3",
    "gh-pages": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
